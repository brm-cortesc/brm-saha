//-Layout Base
extends ../templates/base.pug

//-Etiqueta Title
block title
	-var titulo = "Prensa";
	title Saha - Tienda en línea

//-Etiqueta Metadescription
block description
	meta(name="description", content="Saha - Tienda en línea" )

block content
	container.container-fluid

		div.row
			article.col-lg-10.col-lg-offset-1
				h1.title-prensa prensa

				div.slider.slider-prensa

					-for(x=0; x < 12; x++)
						div.slide
							a(href="images/gallery-prensa-big.jpg")
								+image('demo-gallery-prensa.jpg')(class="img-responsive")


block Scripts
	//-Acá se ponen js que solo vayan en ésta página
	script(src="js/saha.prensa.js")